{
  "manifest_version": 3,
  "name": "Color Changer",
  "version": "1.0",
  "description": "Change specific color in image.",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "logo16.png",
      "48": "logo48.png",
      "128": "logo128.png"
    }
  },
  "permissions": [
    "activeTab",
    "scripting"
  ]
}